<AnchorPane styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sk.badand.beehive.gui.GuiPresenter">
    <stylesheets>
        <URL value="@/styles/gui.css" />
    </stylesheets>
    <children>
        <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <!--<fx:include source="../../../../../resources/fxml/parts/Menu.fxml"/>-->
                
                <AnchorPane fx:id="menu" />
                <AnchorPane fx:id="content" VBox.vgrow="ALWAYS">
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </padding>
                </AnchorPane>
            </children>
        </VBox>
    </children>
</AnchorPane>
